{
  "name": "Blade Runner 2049",
  "type": "dark",
  "colors": {
    "editor.background": "#0c0c0c",
    "editor.foreground": "#ffcc99",
    "editor.lineHighlightBackground": "#1a1a1a",
    "editorCursor.foreground": "#00fff7",
    "editorLineNumber.foreground": "#444",
    "editor.selectionBackground": "#ffaa0033",
    "editor.inactiveSelectionBackground": "#ffaa0011",
    "editorIndentGuide.background1": "#222",
    "editorIndentGuide.activeBackground1": "#ffaa00",
    "sideBar.background": "#0c0c0c",
    "sideBar.foreground": "#cccccc",
    "statusBar.background": "#000000",
    "statusBar.foreground": "#ffaa00",
    "terminal.background": "#0c0c0c",
    "terminal.foreground": "#ffcc99",
    "activityBar.background": "#000000",
    "activityBar.foreground": "#ffaa00",
    "tab.activeBackground": "#0c0c0c",
    "tab.inactiveBackground": "#121212",
    "tab.activeForeground": "#ffaa00",
    "tab.inactiveForeground": "#666666",
    "titleBar.activeBackground": "#0c0c0c",
    "titleBar.activeForeground": "#ffaa00"
  },
  "tokenColors": [
    {
      "scope": ["comment"],
      "settings": {
        "foreground": "#44475a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["keyword", "storage.type"],
      "settings": {
        "foreground": "#ffaa00",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#00ffe6"
      }
    },
    {
      "scope": ["variable", "identifier"],
      "settings": {
        "foreground": "#ffcc99"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#ff3c3c"
      }
    },
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#ffaa00"
      }
    },
    {
      "scope": ["entity.name.type", "support.class"],
      "settings": {
        "foreground": "#ffcc66",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#00ffff"
      }
    }
  ]
}
